{
  "name": "ip-resolve",
  "version": "1.0.0",
  "description": "Node.js app for IP geolocation resolution",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/server.js",
    "dev": "ts-node src/server.ts",
    "download-db": "ts-node src/download-db.ts",
    "setup": "npm install && npm run download-db",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "express": "^4.18.2",
    "maxmind": "^4.3.8"
  },
  "engines": {
    "node": ">=22"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
