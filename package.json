{
  "name": "ip-resolve",
  "version": "1.0.0",
  "description": "Node.js app for IP geolocation resolution",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/server.js",
    "dev": "ts-node src/server.ts",
    "download-db": "ts-node src/download-db.ts",
    "setup": "npm install && npm run download-db",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "express": "^4.22.1",
    "ipaddr.js": "^2.2.0",
    "maxmind": "^4.3.29"
  },
  "engines": {
    "node": ">=22"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.10.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
